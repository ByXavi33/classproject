package first;

public class Member {

	public static void main(String[] args) {
		
		System.out.println("hello java!");
		
		//자기 자신의 이름을 출력하는 메소드
		//반환타입 : 없다 => void
		//매개변수 : 없다
		//처리내용 : 자신의 이름을 출력한다.
		void printMyName() {
		
			System.out.println("안녕하세요 정요셉 입니다.");
			
			//반환이 있다 -> return 사용해서 데이터 반환
			
		}
		
		
		
		System.out.println("");
	}
}
